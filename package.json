{
  "name": "@resourge/react-search-params",
  "version": "1.6.0",
  "description": "react-search-params is a simple hook to control current url search. Also provides so methods that extend URLSearchParams.",
  "main": "./dist/main.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/react-search-params.production.min.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "URLSearchParams",
    "react",
    "hooks",
    "typescript",
    "react-hooks",
    "react-search-params"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/react-search-params.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "scripts": {
    "commit": "git pull && git add . && git-cz",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "fix-types": "node ./scripts/FixTypes.js",
    "dev": "vite",
    "build": "rollup -c && npm run fix-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-newlines": "^1.2.3",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jsdom": "^20.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  }
}
