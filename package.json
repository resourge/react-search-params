		"react",
		"hooks",
		"typescript",
		"react-hooks",
		"react-search-params"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/react-search-params.git"
	},
	"scripts": {
